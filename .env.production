# Configuração de produção para servidor próprio
# Sistema VAI - Produção sem dependências terceiras

# URL da API (será substituída pelo script de deploy)
VITE_API_URL=https://api.vendedorautomaticointeligente.com/api
VITE_ENVIRONMENT=production
VITE_API_TIMEOUT=30000
VITE_BASE_URL=https://api.vendedorautomaticointeligente.com
VITE_APP_NAME=VAI - Vendedor Automático Inteligente
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_SENTRY=false
VITE_AUTH_STORAGE_KEY=vai_auth_token
VITE_AUTH_REFRESH_INTERVAL=3600000
VITE_DEBUG=false
VITE_LOG_LEVEL=info
